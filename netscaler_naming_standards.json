{
  "global_notes": {
    "section_name_readable": "Global Notes (Not Objects)",
    "datacenter_tagging": {
      "object_name_readable": "Datacenter Tagging",
      "prefix": "N/A",
      "middle": "N/A",
      "suffix": "N/A",
      "example": "N/A",
      "notes": "In multi-site deployments, use a short site or datacenter tag (e.g., -bl, -bc) immediately before the protocol/port suffix on site-local objects (vServers, Services, Service Groups). Refer to individual object notes for placement."
    },
    "case_format": {
      "object_name_readable": "Case & Format",
      "prefix": "N/A",
      "middle": "N/A",
      "suffix": "N/A",
      "example": "cx-vs-citrix.com-443",
      "notes": "All names must be lowercase and adhere to the {prefix}-{middle}-{suffix} structure. Environment tags are unnecessary since each environment has its own VPX."
    }
  },
  "content_switching_cs": {
    "section_name_readable": "Content Switching (CS)",
    "content_switching_action": {
      "object_name_readable": "Content Switching Action",
      "prefix": "cx-act-",
      "middle": "<service or FQDN>",
      "suffix": "-<protocol>",
      "example": "cx-act-citrix.contoso.com-ssl",
      "notes": "Protocol suffix indicates target protocol (ssl, http, tcp)."
    },
    "content_switching_policy": {
      "object_name_readable": "Content Switching Policy",
      "prefix": "cx-pol-",
      "middle": "<match-logic>",
      "suffix": "",
      "example": "cx-pol-host-citrix.contoso.com",
      "notes": "Matching logic such as hostname or path."
    },
    "content_switching_vserver": {
      "object_name_readable": "Content Switching vServer",
      "prefix": "cx-vs-",
      "middle": "<service or FQDN>",
      "suffix": "-<port>",
      "example": "cx-vs-citrix.contoso.com-443",
      "notes": "Port suffix shows listener port; see Global Notes for optional datacenter tag."
    },
    "content_switching_policy_label": {
      "object_name_readable": "Content Switching Policy Label",
      "prefix": "cx-pl-",
      "middle": "<service-grouping-function>",
      "suffix": "",
      "example": "cx-pl-citrix-host-to-path",
      "notes": "Used for grouped or chained policies. **Include the function** (e.g., host-to-path, security-checks) for flow clarity."
    }
  },
  "load_balancing_lb": {
    "section_name_readable": "Load Balancing (LB)",
    "load_balancing_vserver": {
      "object_name_readable": "Load Balancing vServer",
      "prefix": "lb-vs-",
      "middle": "<service or FQDN>",
      "suffix": "-<protocol>",
      "example": "lb-vs-studio.test.contoso.com-ssl",
      "notes": "Mirrors Content Switching naming. Protocol suffix usually ssl, http, tcp; see Global Notes for optional datacenter tag."
    }
  },
  "ssl_objects": {
    "section_name_readable": "SSL Objects",
    "ssl_profile_front_end": {
      "object_name_readable": "SSL Profile (Front-End)",
      "prefix": "ssl-",
      "middle": "<service or FQDN>",
      "suffix": "-fe",
      "example": "ssl-citrix.contoso.com-fe",
      "notes": "Used for client-facing vServers (CS/LB front-end)."
    },
    "ssl_profile_back_end": {
      "object_name_readable": "SSL Profile (Back-End)",
      "prefix": "ssl-",
      "middle": "<service or FQDN>",
      "suffix": "-be",
      "example": "ssl-citrix.contoso.com-be",
      "notes": "Used for server-facing SSL services or service groups (re-encryption)."
    },
    "shared_ssl_profile": {
      "object_name_readable": "Shared SSL Profile",
      "prefix": "ssl-shared-",
      "middle": "<purpose>",
      "suffix": "",
      "example": "ssl-shared-modern",
      "notes": "Generic reusable profile for standardized cipher/protocol settings."
    },
    "certificate_file": {
      "object_name_readable": "Certificate File",
      "prefix": "crt-",
      "middle": "<fqdn>",
      "suffix": "-<expiration_year>",
      "example": "crt-citrix.contoso.com-2026.pem",
      "notes": "Certificate exported from PKI. FQDN identifies the service; suffix is expiration year."
    },
    "private_key_file": {
      "object_name_readable": "Private Key File",
      "prefix": "key-",
      "middle": "<fqdn>",
      "suffix": "-<expiration_year>",
      "example": "key-citrix.contoso.com-2026.key",
      "notes": "Private key paired with the certificate; same middle/suffix as certificate for pairing."
    },
    "certificate_key_pair": {
      "object_name_readable": "Certificate-Key Pair",
      "prefix": "ckp-",
      "middle": "<fqdn>",
      "suffix": "-<protocol>",
      "example": "ckp-citrix.contoso.com-ssl",
      "notes": "NetScaler object referencing certificate and key; protocol usually ssl."
    }
  },
  "service_groups_monitors": {
    "section_name_readable": "Service Groups / Services / Monitors",
    "service_group": {
      "object_name_readable": "Service Group",
      "prefix": "sg-",
      "middle": "<service or FQDN>",
      "suffix": "-<protocol>",
      "example": "sg-citrix.contoso.com-ssl",
      "notes": "Groups backend servers for a service. UDP groups use leading u before port, e.g., -u123; see Global Notes for optional datacenter tag."
    },
    "monitor": {
      "object_name_readable": "Monitor",
      "prefix": "mon-",
      "middle": "<type>-<service or FQDN>",
      "suffix": "",
      "example": "mon-https-citrix.contoso.com",
      "notes": "Monitor type = http, https, tcp, etc."
    },
    "service": {
      "object_name_readable": "Service",
      "prefix": "svc-",
      "middle": "<server>-<service>",
      "suffix": "-<protocol>",
      "example": "svc-srv1-citrix-ssl",
      "notes": "Used when not using service groups; references a single backend server; see Global Notes for optional datacenter tag."
    }
  },
