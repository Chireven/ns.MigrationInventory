  "rewrite": {
    "section_name_readable": "Rewrite",
    "rewrite_action": {
      "object_name_readable": "Rewrite Action",
      "prefix": "rw-act-",
      "middle": "<purpose or fqdn>",
      "suffix": "-<scope>",
      "example": "rw-act-studio.test.contoso.com-url",
      "notes": "Scope identifies what is rewritten (url, header, body, cookie)."
