      "notes": "GSLB domain used for DNS queries."
    },
    "gslb_persistence_group": {
      "object_name_readable": "GSLB Persistence Group",
      "prefix": "gslb-pg-",
      "middle": "<group-name>",
      "suffix": "",
      "example": "gslb-pg-citrix",
      "notes": "Defines persistence (stickiness) behavior across GSLB sites."
    },
    "gslb_parameter_set": {
      "object_name_readable": "GSLB Parameter Set",
      "prefix": "gslb-ps-",
      "middle": "<parameter-set-name>",
      "suffix": "",
      "example": "gslb-ps-default",
      "notes": "Controls global GSLB behavior and defaults."
    },
    "gslb_policy": {
      "object_name_readable": "GSLB Policy",
      "prefix": "gslb-pol-",
      "middle": "<policy-name>",
      "suffix": "",
