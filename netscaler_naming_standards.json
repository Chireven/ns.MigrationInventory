      "notes": "Used to chain or group GSLB policies. **Include the function** (e.g., pre-checks, failover) for flow clarity."
    }
  },
  "authoritative_dns_adns": {
    "section_name_readable": "Authoritative DNS (ADNS)",
    "dns_vserver": {
      "object_name_readable": "DNS vServer",
      "prefix": "dns-vs-",
      "middle": "<service or FQDN>",
      "suffix": "-<port>",
      "example": "dns-vs-dns.contoso.com-53",
      "notes": "Authoritative DNS vServer; always append port (53 for UDP/TCP)."
    },
    "dns_service": {
      "object_name_readable": "DNS Service",
      "prefix": "dns-svc-",
      "middle": "<service or FQDN>",
      "suffix": "-<port>",
      "example": "dns-svc-dns.bl.contoso.com-53",
      "notes": "Backend DNS service on a specific node; include port to distinguish UDP/TCP if needed."
    },
    "dns_policy": {
      "object_name_readable": "DNS Policy",
      "prefix": "dns-pol-",
      "middle": "<match-logic>",
      "suffix": "",
      "example": "dns-pol-zone-contoso.com",
      "notes": "Defines when to apply particular DNS behavior or view; match logic usually zone or query type."
    },
    "dns_monitor": {
      "object_name_readable": "DNS Monitor",
      "prefix": "dns-mon-",
      "middle": "<type>-<service or FQDN>",
      "suffix": "",
      "example": "dns-mon-udp-dns.contoso.com",
      "notes": "Health monitoring for DNS backends; type usually udp or tcp."
    },
    "dns_zone_file": {
      "object_name_readable": "DNS Zone File",
      "prefix": "dns-zone-",
      "middle": "<zone-name>",
      "suffix": "",
      "example": "dns-zone-contoso.com",
      "notes": "Represents a zone; zone names should be lowercase. Dots allowed only as part of the FQDN."
    },
    "dns_parameter_set": {
      "object_name_readable": "DNS Parameter Set",
      "prefix": "dns-ps-",
      "middle": "<parameter-set-name>",
      "suffix": "",
      "example": "dns-ps-default",
      "notes": "DNS-wide settings like default TTLs; align with GSLB and site logic. Always append port/protocol on vServers/Services to clearly identify transport."
    }
  },
  "policy_logic_components": {
    "section_name_readable": "Policy Logic Components",
    "named_expression_pattern_set": {
      "object_name_readable": "Named Expression / Pattern Set",
