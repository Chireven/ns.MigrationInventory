    "rewrite_policy": {
      "object_name_readable": "Rewrite Policy",
      "prefix": "rw-pol-",
      "middle": "<[matchtype-]service>",
      "suffix": "-<target-vserver>",
      "example": "rw-pol-host-studio-lb-vs-studio.test.contoso.com-ssl",
      "notes": "matchtype is optional (host, path, hostpath, header, cookie, query, method). target-vserver uses existing CS/LB naming."
